{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"editorconfig": true,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "strict",
	"jsxSingleQuote": false,
	"overrides": [
		{ // Prettier's .editorconfig support apparently only reads [*].
			"files": ["*.yml", "*.yaml"],
			"options": {
				"tabWidth": 2,
				"useTabs": false
			}
		}
	],
	"printWidth": 100,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"semi": true,
	"singleAttributePerLine": false,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "all",
	"useTabs": true,
	"vueIndentScriptAndStyle": false
}
